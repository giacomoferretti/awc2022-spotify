{
  "name": "awc2022-spotify",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "run-s build preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "run-s clean:dist clean:stats clean:coverage",
    "clean:dist": "rimraf ./dist",
    "clean:stats": "rimraf ./stats.html",
    "clean:coverage": "rimraf ./coverage",
    "ts:watch": "tsc --watch"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "classnames": "^2.3.1",
    "history": "^5.3.0",
    "i18next": "^21.8.10",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.33.0",
    "react-i18next": "^11.17.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.15.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-message-helpers": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "reduce-function-call": "^1.0.3",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.15.2"
  }
}
